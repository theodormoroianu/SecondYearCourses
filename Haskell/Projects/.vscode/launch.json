    "version": "0.2.0",
    "configurations": [
        {
            "type": "ghc",
            "request": "launch",
            "name": "haskell-debug-adapter",
            "internalConsoleOptions": "openOnSessionStart",
            "workspace": "${workspaceFolder}",
            "startup": "/home/theodor/Projects/SecondYearCourses/Haskell/Projects/ex.hs",
            "startupFunc": "f",
            "startupArgs": "M [L [1, 2, 3], L [3, 4, 5]]",
